class Handler
  def error_test(num)
    int i = num
    int j = 0

    begin
      raise(ArgumentError, "Horrible error.") if i == 1
      raise(LoadError, "Bad error.") if i == 2
      raise(SyntaxError, "Disgusting error.") if i == 3
    rescue ArgumentError
      j = 1
    rescue LoadError
      j = 2
    rescue SyntaxError
      j = 3
    else
      print "hi!"
      j = 4
    ensure
      j += 5
    end

    return j
  end
end
