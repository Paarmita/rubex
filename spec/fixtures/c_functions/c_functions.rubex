cdef object this_is_it
  return "this is it."
end

class CFunctions
  def pure_ruby_method
    int a = 55
    b = 5.43
    int c = first_c_function(a, b)
    d = this_is_it

    return a + c
  end

  cdef int first_c_function(int a, float b)
    int c = a + 5
    int d = (c * b + 3)/5

    return c - d
  end
end
